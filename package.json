{
  "name": "inbox-template",
  "version": "0.1.0",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "normalize.css": "^8.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "starter-scripts": "^3.4.4"
  },
  "scripts": {
    "start": "starter-scripts start",
    "build": "starter-scripts build",
    "test": "starter-scripts test --env=jsdom",
    "eject": "starter-scripts eject",
    "deploy": "yarn run build && source scripts/deploy.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
